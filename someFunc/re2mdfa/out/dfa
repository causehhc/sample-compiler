digraph NFA{
  rankdir=LR;
  node [shape=circle];
  edge [color=black];
	A -> B[label=a];
	B -> D[label=a];
	A -> C[label=b];
	C -> E[label=b];
	E -> F[label=c];
	F -> F[label=c];
D [shape=doublecircle];E [shape=doublecircle];F [shape=doublecircle];x [label= "start", shape=none,height=.0,width=.0];x -> A;
}
